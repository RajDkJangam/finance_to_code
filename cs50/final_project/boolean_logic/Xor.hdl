/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=a1);
    Not(in=b, out=b1);
    And(a=a, b=b1, out=s1);
    And(a=a1, b=b, out=s2);
    Or(a=s1, b=s2, out=out);
}